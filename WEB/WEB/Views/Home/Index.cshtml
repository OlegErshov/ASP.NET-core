@{
    ViewBag.Title = "Index";
}

    <main class="container">
        <h1>Лабораторная работа 1</h1>
        <h2>Список</h2>

        <ol>
            <li>Элемент 1</li>
            <li>Элемент 2</li>
            <li>Элемент 3</li>
        </ol>

        <hr />

        <h2>Форма</h2>

        <form>
            <fieldset class="row mb-3">
                <div class="col-sm-10">
                    <div class="form-check">
                        <input class ="form-check-input" type="checkbox" name="чек-бокс 1" />
                        <label class ="form-check-label">чек-бокс 1</label>
                    </div>

                    <div class="form-check">
                        <input class="form-check-input" type="checkbox" name="чек-бокс 2" />
                        <label class="form-check-label">чек-бокс 2</label>
                    </div>

                   
                   
                    
                </div>
                <div class="col-sm-12">
                    <div class="form-check">
                        <input class="form-check-input" type="radio" name="radio1" />
                        <label class="form-check-label">radio-1</label>
                    </div>


                </div>
                <div class="col-sm-12">
                    <div class="form-cheak">
                        <input class="form-check-input" type="radio" name="radio2" />
                        <label class="form-check-label">radio-2</label>
                    </div>
                </div>
                <div class="col-sm-12">
                    <div class="form-cheak">
                        <input class="form-check-input" type="radio" name="radio3" />
                        <label class="form-check-label">radio-3</label>
                    </div>
                </div>
            </fieldset>
           

            <div class="row mb-3">
                <label for="inputEmail3" class="col-sm-2 col-form-label">Email</label>
                <div class="col-sm-10">
                    <input type="email" class="form-control" id="inputEmail3">
                </div>
            </div>
            <div class="row mb-3">
                <label for="inputPassword3" class="col-sm-2 col-form-label">Password</label>
                <div class="col-sm-10">
                    <input type="password" class="form-control" id="inputPassword3">
                </div>
            </div>

            <button type="submit" class="btn btn-primary">Отправить</button>
        </form>
    </main>
  